<policies>
  <inbound>
    <base />
    <rate-limit calls="${rate_limit_emd}" renewal-period="60" />
    <include-fragment fragment-id="emd-validate-token-mil" />
  </inbound>
  <backend>
    <base />
  </backend>
  <outbound>
    <base />
  </outbound>
  <on-error>
    <base />
    <return-response>
      <set-status code="500" reason="Policy Error" />
    </return-response>
  </on-error>
</policies>
