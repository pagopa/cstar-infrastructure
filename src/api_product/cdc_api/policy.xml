<policies>
  <inbound>
    <base />
    <set-header name="x-user-id" exists-action="override">
      <value>@(context.User.Id)</value>
    </set-header>
    <set-header name="x-apim-request-id" exists-action="override">
      <value>@(context.RequestId.ToString())</value>
    </set-header>
  </inbound>
  <backend>
    <base />
  </backend>
  <outbound>
    <base />
  </outbound>
  <on-error>
    <base />
  </on-error>
</policies>
